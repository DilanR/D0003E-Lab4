#include "include/GUI.h"


void switchState(GUI *this, int arg) {
    if (this->currentP == 0) {
        this->currentP = 4;
    }else if (this->currentP == 4) {
        this->currentP = 0;
    }
}

void printFreq(GUI *this, int arg) {
    int pp = this->currentP;
	writeChar( (num % 100) / 10 + '0', pp);
	pp++;
	writeChar( num % 10 + '0', pp);
}
